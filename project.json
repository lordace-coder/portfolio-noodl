dia Query Setup","id":"5004b0c7-9f53-2ad1-17ce-34c2958e6a0f","graph":{"connections":[],"roots":[{"id":"68b8d715-d75e-7c09-0d3a-e53fded87b02","type":"Javascript2","label":"Register media queries","x":103,"y":-17,"parameters":{"code":"const queryObjects = [\n    {\n        \"name\": \"Small Mobile\",\n        \"query\": \"(max-width: 400px)\"\n    },\n    {\n        \"name\": \"Regular Mobile\",\n        \"query\": \"(min-width: 401px) and (max-width: 700px)\"\n    },\n    {\n        \"name\": \"Tablet\",\n        \"query\": \"(min-width: 701px) and (max-width: 1200px)\"\n    },\n    {\n        \"name\": \"Regular Desktop\",\n        \"query\": \"(min-width: 1201px) and (max-width: 1800px)\"\n    },\n    {\n        \"name\": \"Large Desktop\",\n        \"query\": \"(min-width: 1801px)\"\n    }\n]\n\nNode.OnInit = function() {\n  // Create a variable to hold global queries\n  Noodl.Variables['Active Media Queries'] = []\n\n  this.createdQueries = []\n  this.queries = {}\n  this.listeners = {}\n\n  this.createListener = (queryObject) => {\n    const qId = queryObject.name\n\n    this.listeners[qId] = query => {\n      if (query.matches) {\n        console.log(qId)\n        Noodl.Variables['Active Media Queries'].push(qId)\n      } else {\n        Noodl.Variables['Active Media Queries'] = Noodl.Variables['Active Media Queries'].filter(q => q !== qId)\n      }\n    }\n\n    this.createdQueries.push(qId)\n    this.queries[qId] = window.matchMedia(queryObject.query)\n    this.listeners[qId](this.queries[qId])\n    this.queries[qId].addEventListener('change', this.listeners[qId])\n  }\n\n  queryObjects.forEach((queryObject) => {\n    this.createListener(queryObject)\n  })    \n}\n\nNode.OnDestroy = function() {\n    if(this.createdQueries.length) {\n      this.createdQueries.forEach(qId => {\n        this.queries[qId].removeEventListener('change', this.listeners[qId])\n        this.queries[qId] = undefined\n        this.listeners[qId] = undefined\n        this.createdQueries